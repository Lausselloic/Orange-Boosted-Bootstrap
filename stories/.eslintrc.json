{
  "env": {
    "browser": false,
    "node": true
  },
  "parserOptions": {
    "sourceType": "script"
  },
  "extends": "../.eslintrc.json",
  "rules": {
    "no-await-in-loop": "off",
    "no-console": "off",
    "no-multi-str": "off",
    "strict": "error",
    "unicorn/prefer-top-level-await": "off"
  }
}
